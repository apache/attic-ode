<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<system-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.fivesight.com/pxe/system-descriptor/ http://www.fivesight.com/pxe/system-descriptor/"
 xmlns="http://www.fivesight.com/pxe/system-descriptor/"
 name="HelloWorld"
 wsdlUri="uri:hello.wsdl">
  <channels>
    <channel name="channel1" />
    <channel name="channel2" />
  </channels>
  <services>
        <service name="HelloService" 
                 provider="uri:protocoladapter.soap"
                 xmlns:foo="bar">
            <imports>
                <port  xmlns:hw="http://pxe/bpel/unit-test.wsdl"
                       name="HelloPort1"
                       type="hw:HelloPortType"
                       channel-ref="channel1"/>
                <port  xmlns:hw2="http://pxe/bpel/unit-test2.wsdl"
                       name="HelloPort2" type="hw2:HelloPortType"
                       channel-ref="channel2"/>
            </imports>
        </service>
        <service name="helloWorld.BpelService"
                 provider="uri:bpelProvider"
                 xmlns:bar="foo">
            <exports>
                <port xmlns:hw="http://pxe/bpel/unit-test.wsdl"
                      name="helloPartnerLink.me1"
                      type="hw:HelloPortType"
                      channel-ref="channel1" />
                <port  xmlns:hw2="http://pxe/bpel/unit-test2.wsdl"
                       name="helloPartnerLink.me2"
                       type="hw2:HelloPortType"
                       channel-ref="channel2"/>
            </exports>
        </service>
  </services>
</system-descriptor>
