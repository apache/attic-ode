<!--
<assign> Test Case
- FROM: location in complex element part
- FROM: expression
- TO: location in complex element part
- TO: to simple type
- EXPR: getVariableData to obtain simple data
-->

<process name="assign2-2.0"
	 targetNamespace="http://ode/bpel/unit-test"
	 xmlns:bpws="http://schemas.xmlsoap.org/ws/2004/03/business-process/"
	 xmlns="http://schemas.xmlsoap.org/ws/2004/03/business-process/"
	 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	 xmlns:test="uri:testing"
	 xmlns:t="uri:testing">

  <import location="../test.wsdl"
	  importType="http://schemas.xmlsoap.org/wsdl/"
	  namespace="uri:testing" />

  <partnerLinks>
    <partnerLink name="testPartnerLink"
		 partnerLinkType="test:PartnerLinkType1"
		 myRole="me" />
  </partnerLinks>
  
  <variables>
    <variable name="var1" messageType="test:Message1"/>
    <variable name="var2" type="xsd:string"/>
  </variables>
  
  <sequence>
    <receive
      createInstance="yes"
      name="startReceive"
      partnerLink="testPartnerLink"
      portType="test:PortType1"
      operation="opTwoWay"
      variable="var1"/>

    <assign name="b">
      <copy>
        <from>$var1.Complex1Part1/t:StringElement</from>
        <to>$var2</to>
      </copy>
      <copy>
        <from>concat($var2,'def')</from>
        <to>$var1.Complex1Part1/t:StringElement</to>
      </copy>
    </assign>

    <reply name="endReply"
      operation="opTwoWay"
      partnerLink="testPartnerLink"
      portType="test:PortType1"
      variable="var1"/>

  </sequence>

</process>
