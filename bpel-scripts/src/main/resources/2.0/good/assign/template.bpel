	     createInstance="yes"
	     partnerLink="testPartnerLink"
	     portType="test:PortType1"
	     operation="opTwoWay"
	     variable="var1"/>

    <assign name="assign1">
      <!-- COPIES GO HERE -->
    </assign>

    <reply name="endReply"
      operation="opTwoWay"
      partnerLink="testPartnerLink"
      portType="test:PortType1"
      variable="var1"/>

  </sequence>

</process>
