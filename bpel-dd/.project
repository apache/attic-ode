<?xml version="1.0" encoding="UTF-8"?>
<projectDescription>
	<name>ode-bpel-dd</name>
	<comment>Contains the deployment descriptor classes used to validate the descriptor during compilation and read it on deployment.</comment>
	<projects>
		<project>ode-bpel-obj</project>
		<project>ode-utils</project>
	</projects>
	<buildSpec>
		<buildCommand>
			<name>org.eclipse.ui.externaltools.ExternalToolBuilder</name>
			<triggers>full,incremental,</triggers>
			<arguments>
				<dictionary>
					<key>LaunchConfigHandle</key>
					<value>&lt;project&gt;/.externalToolBuilders/org.eclipse.jdt.core.javabuilder.launch</value>
				</dictionary>
			</arguments>
		</buildCommand>
	</buildSpec>
	<natures>
		<nature>org.eclipse.jdt.core.javanature</nature>
	</natures>
</projectDescription>
