<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app PUBLIC
	"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Intalio PXE SOAP Service Provider (HTTP Adapter)</display-name>
  <servlet>
    <servlet-name>SoapHttpAdapter</servlet-name>
    <display-name>Intalio PXE HTTP-SOAP Servlet</display-name>
    <servlet-class>com.fs.pxe.httpsoap.HttpSoapServlet</servlet-class>
    <init-param>
       <param-name>ConnectionProperties</param-name>
       <param-value>URL=rmi://localhost:${pxe.rmi.port}/${pxe.rmi.domain}</param-value>
    </init-param>
    <init-param>
       <param-name>ServiceProviderURI</param-name>
       <param-value>uri:protocoladapter.soap</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SoapHttpAdapter</servlet-name>
    <url-pattern>/soap/*</url-pattern>
  </servlet-mapping>

</web-app>
