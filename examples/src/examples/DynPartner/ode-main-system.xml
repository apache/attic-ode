<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<system-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.fivesight.com/ode/system-descriptor/ http://www.fivesight.com/ode/system-descriptor/"
                   xmlns:hw="http://ode/bpel/unit-test.wsdl"
                   xmlns:resp="http://ode/bpel/responder.wsdl"
                   xmlns="http://www.fivesight.com/ode/system-descriptor/"
                   name="DynMain"
                   wsdlUri="urn:/Main.wsdl">
  <channels>
    <channel name="inboundChannel" />
    <channel name="outboundChannel" />
  </channels>
  <services>
    <service name="DynMainService" provider="uri:protocoladapter.soap">
      <imports>
        <port name="DynMainPort" type="hw:DynMainPortType" channel-ref="inboundChannel" />
      </imports>
    </service>
    <service name="DynMainService.OutboundSoapService" provider="uri:protocoladapter.soap">
      <properties>
        <property name="serviceWsdlNamespace" value="http://ode/bpel/responder.wsdl"/>
        <property name="serviceWsdlName" value="DynResponderService"/>
      </properties>
      <exports>
        <port name="DynResponderPort" type="resp:DynResponderPortType" channel-ref="outboundChannel"/>
      </exports>
    </service>
    <service name="dynMain.BpelService" provider="uri:bpelProvider">
      <properties>
        <property name="compiledProcess" value="DynPartnerMain.cbp"/>
      </properties>
      <imports>
        <port name="responderPartnerLink.responder" type="resp:DynResponderPortType" channel-ref="outboundChannel" />
      </imports>
      <exports>
        <port name="initiatorPartnerLink.me" type="hw:DynMainPortType" channel-ref="inboundChannel" />
      </exports>
    </service>
  </services>
</system-descriptor>
