<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<system-descriptor name="SaajConnectorWsiTest"
  wsdlUri="MUST_BE_SET_BY_TESTCASE"
  xmlns="http://www.fivesight.com/pxe/system-descriptor/"
  xmlns:scm="http://www.ws-i.org/SampleApplications/SupplyChainManagement/2002-08/Retailer.wsdl">
  <channels>
    <channel name="channel1"/>
  </channels>
  <services>
    <service name="MockAdapter" provider="uri:MockProtocolAdapter">
      <imports>
        <port name="HelloPort" type="scm:RetailerPortType" channel-ref="channel1"/>
      </imports>
    </service>

    <service name="RetailerService" provider="uri:SaajConnector">
      <exports>
        <port name="RetailerPort" type="scm:RetailerPortType" channel-ref="channel1" />
      </exports>
    </service>
  </services>

</system-descriptor>
