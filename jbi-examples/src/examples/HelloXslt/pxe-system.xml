<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<system-descriptor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.fivesight.com/pxe/system-descriptor/ http://www.fivesight.com/pxe/system-descriptor/"
 xmlns:hw="http://pxe/bpel/unit-test.wsdl"
 xmlns="http://www.fivesight.com/pxe/system-descriptor/"
 name="HelloXslWorld"
 wsdlUri="urn:/HelloXslWorld.wsdl">
    <channels>
    <channel name="channel1" />
  </channels>
  <services>
        <service name="HelloXslService" 
            provider="uri:protocoladapter.soap">
            <imports>
                <port name="HelloXslPort" type="hw:HelloXslPortType" channel-ref="channel1" />
            </imports>
        </service>
        <service name="helloXslWorld.BpelService" provider="uri:bpelProvider">
          <properties>
             <property name="compiledProcess" value="HelloXslWorld.cbp"/>
           </properties>
            <exports>
                <port name="helloXslPartnerLink.me" type="hw:HelloXslPortType" channel-ref="channel1" />
            </exports>
        </service>
  </services>
</system-descriptor>
